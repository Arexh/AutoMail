  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "productName": "AutoMail",
    "appId": "sustech.shude.automail",
    "directories": {
      "output": "out"
    },
    "mac": {
      "target": "dmg"
    },
    "dmg": {
      "backgroundColor": "#fff"
    },
    "extends": null,
    "files": [
      "build/**/*",
      "public/main.js",
      "public/preload.js"
    ]
  }
}
