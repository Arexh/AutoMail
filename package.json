    "@loadable/component": "^5.13.2",
    "@turf/turf": "^6.5.0",
    "arco-design-pro": "^2.7.0",
    "axios": "^0.27.2",
    "axios-cookiejar-support": "^4.0.3",
    "bizcharts": "^4.1.11",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dexie": "^3.2.2",
    "dexie-react-hooks": "^1.1.1",
    "electron-reload": "^2.0.0-alpha.1",
    "fuse.js": "^6.6.2",
    "http-proxy-middleware": "^2.0.6",
    "is-electron": "^2.2.1",
    "lodash": "^4.17.21",
